{
  "extends": "@docusaurus/module-type-aliases",
  "compilerOptions": {
    "baseUrl": ".",
    "strict": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "target": "ESNext",
    "lib": ["DOM", "ES2022"],
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "include": ["src/**/*", "docusaurus.config.ts", "sidebars.ts"]
}
